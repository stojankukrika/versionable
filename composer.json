{
    "name": "stojankukrika/versionable",
    "license": "MIT",
    "description": "Allows to create Laravel 4 / 5 / 6 / 7 /8 Model versioning and restoring",
    "keywords": [
        "model",
        "laravel",
        "ardent",
        "version",
        "history",
        "restore"
    ],
    "homepage": "http://github.com/stojankukrika/versionable",
    "authors": [
        {
            "name": "Stojan Kukrika",
            "email": "stojan.kukrika@gmail.com"
        }
    ],
    "support": {
        "issues": "https://github.com/stojankukrika/versionable/issues",
        "source": "https://github.com/stojankukrika/versionable"
    },
    "require": {
        "php": "^7.1 || ^7.2 || ^7.3 || ^7.4",
        "illuminate/support": "~5.3 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "7.* || ^8.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^3.7"
    },
    "autoload": {
        "classmap": [
            "src/migrations",
            "tests/VersionableTestCase.php"
        ],
        "psr-0": {
            "StojanKukrika\\Versionable": "src/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "StojanKukrika\\Versionable\\Providers\\ServiceProvider"
            ]
        }
    }
}
